<document>
  <menuBarTemplate>
    <menuBar>
    {{#each menuItems}}
      <menuItem route="{{this.route}}" autoHighlight="{{this.autoHighlight}}">
        <title>{{formatMessage (intlGet this.title)}}</title>
      </menuItem>
    {{/each}}
    </menuBar>
  </menuBarTemplate>
</document>
