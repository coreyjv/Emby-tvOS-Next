<?xml version="1.0" encoding="UTF-8" ?>
<document>
	<head>
		<style>{{> styles}}</style>
	</head>
		<stackTemplate>
				<banner>
						<title>{{formatMessage (intlGet "messages.allmovies.allmovies")}}</title>
				</banner>
				<collectionList>
						<grid>
								<prototypes>
                  {{> moviePrototype playedCheckmarkClass="played-checkmark"}}
								</prototypes>
								<section binding="items:{movies};" />
						</grid>
				</collectionList>
		</stackTemplate>
</document>